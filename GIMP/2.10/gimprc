# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(language "")
(plug-in-path "${gimp_plug_in_dir}/plug-ins")
(module-path "${gimp_plug_in_dir}/modules")
(interpreter-path "${gimp_plug_in_dir}/interpreters")
(environ-path "${gimp_plug_in_dir}/environ")
(brush-path "${gimp_data_dir}/brushes")
(brush-path-writable "${gimp_data_dir}/brushes")
(dynamics-path "${gimp_data_dir}/dynamics")
(dynamics-path-writable "${gimp_data_dir}/dynamics")
(mypaint-brush-path "${gimp_installation_dir}/share/mypaint-data/1.0/brushes")
(mypaint-brush-path-writable "${gimp_installation_dir}/share/mypaint-data/1.0/brushes")
(pattern-path "${gimp_data_dir}/patterns")
(pattern-path-writable "${gimp_data_dir}/patterns")
(palette-path "${gimp_data_dir}/palettes")
(palette-path-writable "${gimp_data_dir}/palettes")
(gradient-path "${gimp_data_dir}/gradients")
(gradient-path-writable "${gimp_data_dir}/gradients")
(tool-preset-path-writable "${gimp_dir}/tool-presets:${gimp_data_dir}/tool-presets")
(font-path "${gimp_data_dir}/fonts")
(default-image
    (width 1200)
    (height 1600)
    (unit pixels)
    (xresolution 72.000000)
    (yresolution 72.000000)
    (resolution-unit inches)
    (image-type rgb)
    (precision u8-gamma)
    (color-managed yes)
    (color-profile NULL)
    (fill-type background)
    (comment "Created with GIMP"))
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(cursor-mode tool-icon)
(show-paint-tool-cursor no)
(monitor-xresolution 79.000000)
(monitor-yresolution 79.000000)
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection yes)
    (show-layer-boundary no)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 1.000000 1.000000 1.000000)))
(default-fullscreen-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection yes)
    (show-layer-boundary no)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 1.000000 1.000000 1.000000)))
(toolbox-color-area no)
(toolbox-wilber no)
(theme-path "${gimp_data_dir}/themes")
(theme "System")
(icon-theme-path "${gimp_data_dir}/icons")
(fill-options
    (style solid)
    (antialias yes))
(stroke-options
    (style solid)
    (antialias yes)
    (method line)
    (width 6.000000)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10.000000)
    (dash-offset 0.000000)
    (dash-info 0)
    (emulate-brush-dynamics no))
(script-fu-path "${gimp_data_dir}/scripts")


# end of gimprc
